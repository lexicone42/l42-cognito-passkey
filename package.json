{
  "name": "l42-cognito-passkey",
  "version": "0.20.0",
  "sideEffects": false,
  "description": "AWS Cognito authentication with WebAuthn/Passkey support - self-hosted, configurable",
  "type": "module",
  "main": "src/auth.js",
  "module": "src/auth.js",
  "types": "src/auth.d.ts",
  "exports": {
    ".": {
      "import": "./src/auth.js",
      "types": "./src/auth.d.ts"
    }
  },
  "files": [
    "src/auth.js",
    "src/auth.d.ts",
    "dist/auth.js",
    "dist/auth.d.ts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "vitest run plugin/templates/",
    "test:watch": "vitest plugin/templates/",
    "lint": "echo \"No linter configured yet\"",
    "validate-docs": "node scripts/validate-docs.js",
    "check-dist": "node scripts/check-dist-sync.js",
    "sync-dist": "node scripts/check-dist-sync.js --fix",
    "setup-hooks": "node scripts/setup-hooks.js",
    "process-issue": "node scripts/process-issue.js",
    "sync-counts": "node scripts/sync-test-counts.js",
    "preversion": "pnpm test && node scripts/check-dist-sync.js && pnpm validate-docs",
    "version": "node scripts/sync-version.js && node scripts/ensure-changelog.js && node scripts/sync-test-counts.js && cp src/auth.js dist/auth.js && cp src/auth.d.ts dist/auth.d.ts && git add -A",
    "postversion": "git push && git push --tags && node scripts/create-release.js",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major",
    "release:prerelease": "npm version prerelease --preid=rc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexicone42/l42-cognito-passkey.git"
  },
  "keywords": [
    "cognito",
    "aws",
    "authentication",
    "webauthn",
    "passkey",
    "fido2",
    "oauth",
    "jwt"
  ],
  "author": "Lexicone42",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lexicone42/l42-cognito-passkey/issues"
  },
  "homepage": "https://github.com/lexicone42/l42-cognito-passkey#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@cedar-policy/cedar-wasm": "^4.8.2",
    "fast-check": "^4.5.3",
    "jsdom": "^27.4.0",
    "vitest": "^4.0.17"
  }
}
